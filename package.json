    "express": "^4.16.2",
    "hbs": "^4.2.0",
    "knex": "^2.2.0",
    "passport": "^0.6.0",
    "passport-oauth": "^1.0.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "sqlite3": "^5.0.11"
  }
